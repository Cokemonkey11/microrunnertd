package FrozenObeliskKnockup
import UnitApi
import DamageEvent
import Knockback3

constant OB_ID = 'h00Y'

function h()
	let dS = DamageEvent.getSource()
	let tU = DamageEvent.getTarget()

	if GetUnitTypeId(dS) == OB_ID and DamageEvent.getType() == ATTACK and GetUnitAbilityLevel(tU, UnitApi.IMMUNE) < 1
		if Knockback3.getVel(tU).length() == 0.
			Knockback3.add(tU, 500., angle(0.), angle(0.))

init
	DamageEvent.addListener() () ->
		h()
