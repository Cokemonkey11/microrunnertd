package FrozenObeliskKnockup
	import UnitApi
	import DamageType
	import StructuredDD
	import Knockback3D

	constant OB_ID = 'h00Y'

	function h()
		unit dS = GetEventDamageSource()
		unit tU = GetTriggerUnit()

		if GetUnitTypeId(dS) == OB_ID and DamageType.get() == DamageType.ATTACK and GetUnitAbilityLevel(tU, UnitApi.IMMUNE) < 1
			if Knockback3D.getVel(tU) == 0.
				Knockback3D.add(tU, 500., 0., bj_PI/2.)
			end
		end
	end

	init
		StructuredDD.addHandler(function h)
	end
